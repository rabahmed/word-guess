def columnsOf1word2():
    letter1 = input("Enter the column number of 1st word: ")
    if letter1=="1":
        print("A B C D \n")
    elif letter1=="2":
        print("E F G H \n")
    elif letter1=="3":
        print("I G K L \n")
    elif letter1=="4":
        print("M N O P \n")
    elif letter1=="5":
        print("Q R S T \n")
    elif letter1 == "6":
        print("U V W X \n")
    elif letter1 == "7":
        print("Y Z \n")

