def columnsOf1word():
    letter1 = input("Enter the column number of 1st word: ")
    if letter1=="1":
        print("A E I M Q U V \n")
    elif letter1=="2":
        print("B F J N R V Z \n")
    elif letter1=="3":
        print("C G K O S W \n")
    elif letter1=="4":
        print("D H L P T X \n")

