for i in range(1):
    print(" 1 2 3 4")
    print(" A B C D")
    print(" E F G H")
    print(" I J K L")
    print(" M N O P")
    print(" Q R S T")
    print(" U V W X")
    print(" Y Z\n")

len=int(input("Enter the total alphabets of your word: "))
if len==1:
    columnsOf1word()
elif len==2:
    columnsOf2words()
elif len==3:
    columnsOf3words()
elif len==4:
    columnsOf4words()

for i in range(1):
    print("A E I M Q U V")
    print("B F J N R V Z")
    print("C G K O S W")
    print("D H L P T X\n")

